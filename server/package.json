{
  "name": "lumina-outreach-server",
  "version": "1.0.0",
  "description": "Backend server for Lumina Outreach intelligent communication system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node --max-old-space-size=1536 --optimize-for-size --expose-gc dist/index.js",
    "start:prod": "NODE_ENV=production node --max-old-space-size=1536 --optimize-for-size --expose-gc dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "dev:debug": "ts-node-dev --respawn --transpile-only --inspect=0.0.0.0:9229 src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prestart": "npm run build",
    "prestart:prod": "npm run build",
    "docker:build": "docker build -t lumina-outreach-server .",
    "docker:run": "docker run -p 8000:8000 lumina-outreach-server",
    "logs:clear": "rm -f logs/*.log",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "memory:monitor": "node --expose-gc --max-old-space-size=1536 --optimize-for-size dist/index.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@google/genai": "^1.3.0",
    "@google/generative-ai": "^0.24.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "async-retry": "^1.3.3",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.6.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "elevenlabs-node": "^2.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.18.1",
    "express-ws": "^5.0.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.6.1",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "libphonenumber-js": "^1.12.8",
    "mongoose": "^7.8.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^5.0.1",
    "opossum": "^5.0.1",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^7.1.1",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.7.5",
    "streamifier": "^0.1.1",
    "twilio": "^4.12.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.15.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.8.1",
    "@types/connect": "^3.4.38",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.23",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.17.10",
    "@types/express-ws": "^3.0.5",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.2.3",
    "@types/socket.io": "^3.0.1",
    "@types/validator": "^13.11.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
